# Accepts a (int) and b (int) from standard input and writes to standard output the value of
# a^2 + b^2.

0    read     r1          # read a
1    read     r2          # read b
2    mul      r3 r1 r1    # c = a * a
3    mul      r4 r2 r2    # d = b * b
4    add      r5 r3 r4    # e = c + d
5    write    r5          # write e
6    halt                 # halt the machine
