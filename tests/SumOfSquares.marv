# Accepts a (int) and b (int) from standard input and writes to standard output the value of
# a^2 + b^2.

0    read     r1          # r1 = a
1    read     r2          # r2 = b
2    mul      r3 r1 r1    # r3 = r1 * r1
3    mul      r4 r2 r2    # r4 = r2 * r2
4    add      r5 r3 r4    # r5 = r3 + r4
5    write    r5          # write r5
6    halt                 # halt the machine
