# Accepts n (int) from standard input and writes a countdown from n to 0 to standard output.

0    read     r1          # read n
1    jltn     r1 r0 5     # if n < 0 jump to 5
2    write    r1          # write n
3    addn     r1 -1       # n = n - 1
4    jumpn    1           # jump to 2
5    halt                 # halt the machine
